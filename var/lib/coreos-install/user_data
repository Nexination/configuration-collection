#cloud-config

hostname: node01
ssh_authorized_keys:
  - ssh-rsa yoursshkeyhere
coreos:
  units:
    - name: etcd.service
      command: start
    - name: fleet.service
      command: start
  etcd:
    name: node01
    discovery: yourdiscoveryurl
    addr: 192.168.1.10:4001
    peer-addr: 192.168.1.10:7001
